<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="800.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="world.ucode.controller.SelectmenuController">
   <left>
      <Button fx:id="left" mnemonicParsing="false" prefHeight="39.0" prefWidth="59.0" text="&lt;---" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Button fx:id="right" mnemonicParsing="false" prefHeight="39.0" prefWidth="59.0" text="---&gt;" BorderPane.alignment="CENTER" />
   </right>
   <bottom>
      <Button fx:id="Select" mnemonicParsing="false" onAction="#ingame" prefHeight="44.0" prefWidth="72.0" text="Select" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <Button fx:id="Home" mnemonicParsing="false" onAction="#inmainmenu" prefHeight="48.0" prefWidth="59.0" text="Back" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="600.0" />
         </BorderPane.margin>
      </Button>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
